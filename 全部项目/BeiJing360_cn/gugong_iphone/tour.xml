			   border="false"
			   textcolor="0xFFFFFF"
			   effect="glow(0x000000);"
			   textalign="center"
			   />

	<!--鼠标指针-->
    <control movetocursor="arrow" />
	
	<action name="arrowcursor">
		set(control.mousetype, moveto);
		set(cursors.url,  move-cursors.png);
		set(cursors.type, 4way);
		set(cursors.move,      112|0|28|28);
		set(cursors.drag,      112|0|28|28);
		set(cursors.arrow_r,    0|0|28|28);
		set(cursors.arrow_d,   28|0|28|28);
		set(cursors.arrow_l,   56|0|28|28);
		set(cursors.arrow_u,   84|0|28|28);
	</action>
	
	
	
	<!--景区LOGO-->
    <plugin name="logo" url="logo.png" keep="true"  scale="0.9" align="top" zorder="2"  x="0" y="0"   />
	
	
		<!--细节图片热点-->
		<style name="hotspot_click"
	       url="click.png"
	       crop="0|0|105|105"
	       frames="4" framewidth="105" frameheight="105" frame="0"
	       onloaded="style_click(); "
	       altonloaded="if(isphone, mul(scale,2));  onloaded(); "
	       />	

	<action name="style_click"> 
		inc(frame,1,get(frames),0); 
		mul(ypos,frame,frameheight); 
		txtadd(crop,'0|',get(ypos),'|',get(framewidth),'|',get(frameheight)); 
		delayedcall(0.2,style_click() ); 
	</action>
	
	
